
public class MorrisInorder 
{
	public static void inorder(TreeNode root)
	{
		if(root==null)
			return ;
		
		TreeNode curr=root;
		while(curr!=null)
		{
			if(curr.left==null)
			{
				System.out.print(curr.data+" ");
				curr=curr.right;
			}
			else
			{
				// Find Inorder predecessor
			}
		}
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
